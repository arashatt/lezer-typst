@top Program {expression*}
@skip {Comment | Space}
expression {Auto|Boolean|Integer|Float|Length}
@tokens {
  Space {@whitespace+}
  Comment { "//" ![\n]* }
  Auto {"auto"}
  Boolean {"true"|"false"}
  Integer { "0x"(@digit$[a-f])+|@digit+ }
  Float {Integer($[.] Integer|"e"Integer)}
  @precedence {Integer,Length,Float}
  Length {Integer("pt"|"mm"|"cm"|"in")| Float "em"}

}